name:cycleway bridges
filter:"fclass" = 'cycleway'  AND  "bridge" IN ('True', '1', 'yes')
renderer:line
alpha:1
	symbol-class:SimpleLine
	pass:10
		capstyle:round
		customdash:5;2
		customdash_map_unit_scale:0,0,0,0,0,0
		customdash_unit:MapUnit
		draw_inside_polygon:0
		joinstyle:round
		line_color:170,85,127,185
		line_style:dot
		line_width:2
		line_width_unit:MapUnit
		offset:0
		offset_map_unit_scale:0,0,0,0,0,0
		offset_unit:MapUnit
		use_custom_dash:0
		width_map_unit_scale:0,0,0,0,0,0
