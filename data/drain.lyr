name:drain
filter: "fclass"  =  'drain' 
min-scale:500
max-scale:100000
renderer:line
alpha:1
	symbol-class:SimpleLine
	pass:0
		capstyle:square
		customdash:5;2
		customdash_map_unit_scale:0,0,0,0,0,0
		customdash_unit:MM
		draw_inside_polygon:0
		joinstyle:bevel
		line_color:184,217,247,255
		line_style:solid
		line_width:0.3
		line_width_unit:MM
		offset:0
		offset_map_unit_scale:0,0,0,0,0,0
		offset_unit:MM
		use_custom_dash:0
		width_map_unit_scale:0,0,0,0,0,0
